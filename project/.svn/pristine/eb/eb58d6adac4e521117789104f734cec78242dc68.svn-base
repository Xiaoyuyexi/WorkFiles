<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
    
<configuration>
    <!-- 全局别名设置，在映射文件中只需写别名，而不必写出整个类路径  -->
    <typeAliases>  
          <typeAlias type="com.etrack.mybatis.model.Orders" alias="Orders"/> 
          <typeAlias type="com.etrack.mybatis.model.OrderTracking" alias="OrderTracking"/> 
          <typeAlias type="com.etrack.mybatis.model.Channel" alias="Channel"/> 
    </typeAliases> 
    
	<environments default="development">
		<environment id="development">
			<transactionManager type="JDBC" />
			<dataSource type="POOLED">
				<property name="driver" value="com.mysql.jdbc.Driver" />
				<property name="url" value="jdbc:mysql://localhost:3306/wuliu?characterEncoding=UTF-8" />
				<property name="username" value="kxcomm" />
				<property name="password" value="123" />
			</dataSource>
		</environment>
	</environments>

	<mappers>
	 	<mapper resource="com/etrack/mybatis/mapper/OrdersMapper.xml" />
		<mapper resource="com/etrack/mybatis/mapper/OrderTrackingMapper.xml" />
		<mapper resource="com/etrack/mybatis/mapper/ChannelMapper.xml" />
	</mappers>

</configuration>