<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="diycard" extends="global" namespace="/diycard">
		<action name="diycard" class="cn.com.mmsweb.action.web.DiyCardAction">
			<result type="json"><param name="excludeNullProperties">true</param>
				<param name="ignoreHierarchy">false</param>
				<param name="includeProperties">maxPage.*,frameVos.*,photoFrameVo.*,msg,success</param></result>
			<interceptor-ref name="defaultStack"></interceptor-ref>
		</action>
		<action name="cardtype" class="cn.com.mmsweb.action.web.CardTypeAction">
			<result type="json"><param name="excludeNullProperties">true</param>
				<param name="ignoreHierarchy">false</param>
				<param name="includeProperties">frameVos.*,msg,success</param></result>
			<interceptor-ref name="defaultStack"></interceptor-ref>
		</action>
	</package>
</struts>
