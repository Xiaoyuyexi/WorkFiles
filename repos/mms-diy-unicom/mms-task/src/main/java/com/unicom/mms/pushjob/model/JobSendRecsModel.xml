<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.unicom.mms.pushjob.model.JobSendRecsModel">
	<resultMap type="jobSendRecsMode" id="resultJobSendRecsModeMap">
		<result column="Id" property="id" jdbcType="BIGINT"/>
		<result column="create_time" property="createTime"/>
		<result column="music_url" property="musicUrl" jdbcType="VARCHAR"/>
		<result column="send_time" property="sendTime" />
		<result column="pic_url" property="picUrl" jdbcType="VARCHAR"/>
		<result column="send_type" property="sendType" jdbcType="INTEGER"/>
		<result column="reciver" property="reciver" jdbcType="VARCHAR"/>
		<result column="b_success" property="bSuccess" jdbcType="INTEGER"/>
		<result column="push_mdn" property="pushMdn" jdbcType="VARCHAR" /><!-- jdbcType="DATETIME" -->
		<result column="contents" property="contents" jdbcType="VARCHAR"/>
	</resultMap>

	<insert id="saveSendRecs" parameterType="jobSendRecsModel" useGeneratedKeys="true" >
		insert into tb_send_recs(create_time,music_url,pic_url,reciver,b_success,push_mdn,contents)
		values(#{createTime},#{musicUrl},#{picUrl},#{reciver},#{bSuccess},#{pushMdn},#{contents})
	</insert>
	
	
</mapper>