<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
    
<configuration>
    <!-- 全局别名设置，在映射文件中只需写别名，而不必写出整个类路径  -->
    <typeAliases>  
         <typeAlias type="com.unicom.mms.pushjob.model.PolicModel" alias="policModel"/>
         <typeAlias type="com.unicom.mms.pushjob.model.JobSendRecsModel" alias="jobSendRecsMode"/>
    </typeAliases> 
    
	<environments default="development">
		<environment id="development">
			<transactionManager type="JDBC" />
			<dataSource type="POOLED">
				<property name="driver" value="com.mysql.jdbc.Driver" />
				<property name="url" value="jdbc:mysql://localhost:3306/unicom" />
				<property name="username" value="kxcomm" />
				<property name="password" value="123" />
			</dataSource>
		</environment>
	</environments>

	<mappers>
		<mapper resource="com/unicom/mms/pushjob/model/PolicModel.xml" />
		<mapper resource="com/unicom/mms/pushjob/model/JobSendRecsModel.xml"/>
	</mappers>

</configuration>