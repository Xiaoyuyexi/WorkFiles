<?xml version="1.0" encoding="UTF-8"?>
<project
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd"
	xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<modelVersion>4.0.0</modelVersion>
	<parent>
		<artifactId>mms-diy-unicom</artifactId>
		<groupId>com.unicom.mms</groupId>
		<version>1.0-SNAPSHOT</version>
	</parent>
	<groupId>com.unicom.mms</groupId>
	<artifactId>mmsc-gateway</artifactId>
	<version>1.0</version>
	<name>mmsc-gateway</name>
	<url>http://maven.apache.org</url>
	<properties>
		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
		<org.springframework.version>3.1.1.RELEASE</org.springframework.version>
	</properties>
	<dependencies>
		<dependency>
			<groupId>com.unicom.mms</groupId>
			<artifactId>mms-share</artifactId>
			<version>1.0-SNAPSHOT</version>
		</dependency>
		<dependency>
			<!--mms -->
			<groupId>com.unicom</groupId>
			<artifactId>mm7-hn</artifactId>
			<version>1.0</version>
		</dependency>
		<dependency>
			<groupId>org.apache.activemq</groupId>
			<artifactId>activemq-all</artifactId>
			<version>5.8.0</version>
		</dependency>
		<!-- local -->
		<dependency>
		    <groupId>org.apache</groupId>
		    <artifactId>commons-codec</artifactId>
		    <version>1.3</version>
		    <systemPath>${basedir}/lib/commons-codec-1.3.jar</systemPath>
		    <scope>system</scope>
		  </dependency>
		  <dependency>
		    <groupId>org.apache</groupId>
		    <artifactId>commons-codec</artifactId>
		    <version>1.3</version>
		    <systemPath>${basedir}/lib/commons-codec-1.3.jar</systemPath>
		    <scope>system</scope>
		  </dependency>
		  <dependency>
		    <groupId>org.apache</groupId>
		    <artifactId>commons-collections</artifactId>
		    <version>3.2</version>
		    <systemPath>${basedir}/lib/commons-collections-3.2.jar</systemPath>
		    <scope>system</scope>
		  </dependency>
		   <dependency>
		    <groupId>org.apache</groupId>
		    <artifactId>commons-configuration</artifactId>
		    <version>1.6</version>
		    <systemPath>${basedir}/lib/commons-configuration-1.6.jar</systemPath>
		    <scope>system</scope>
		  </dependency>
		  	<dependency>
		    <groupId>org.apache</groupId>
		    <artifactId>commons-lang</artifactId>
		    <version>2.5</version>
		    <systemPath>${basedir}/lib/commons-lang-2.5.jar</systemPath>
		    <scope>system</scope>
		  </dependency>
		  	<dependency>
		    <groupId>org.apache</groupId>
		    <artifactId>commons-logging</artifactId>
		    <version>1.0</version>
		    <systemPath>${basedir}/lib/commons-logging-1.0.jar</systemPath>
		    <scope>system</scope>
		  </dependency>
		  <dependency>
		    <groupId>org.apache</groupId>
		    <artifactId>concurrent</artifactId>
		    <version>1.0</version>
		    <systemPath>${basedir}/lib/concurrent.jar</systemPath>
		    <scope>system</scope>
		  </dependency>
		  	<dependency>
		    <groupId>org.apache</groupId>
		    <artifactId>jdom</artifactId>
		    <version>1.0</version>
		    <systemPath>${basedir}/lib/jdom.jar</systemPath>
		    <scope>system</scope>
		  </dependency>
		  	<dependency>
		    <groupId>org.apache</groupId>
		    <artifactId>log4j</artifactId>
		    <version>1.2.14</version>
		    <systemPath>${basedir}/lib/log4j-1.2.14.jar</systemPath>
		    <scope>system</scope>
		  </dependency>
		  		<dependency>
		    <groupId>org.apache</groupId>
		    <artifactId>servlet</artifactId>
		    <version>1.0</version>
		    <systemPath>${basedir}/lib/servlet-api.jar</systemPath>
		    <scope>system</scope>
		  </dependency>
		  	<dependency>
		    <groupId>mms</groupId>
		    <artifactId>vassdk</artifactId>
		    <version>1.0</version>
		    <systemPath>${basedir}/lib/vassdk_soap.jar</systemPath>
		    <scope>system</scope>
		  </dependency>
		  <dependency>
		    <groupId>mms</groupId>
		    <artifactId>webservices</artifactId>
		    <version>1.0</version>
		    <systemPath>${basedir}/lib/webservices.jar</systemPath>
		    <scope>system</scope>
		  </dependency>
		  	<dependency>
		    <groupId>mms</groupId>
		    <artifactId>xercesImpl</artifactId>
		    <version>1.0</version>
		    <systemPath>${basedir}/lib/xercesImpl.jar</systemPath>
		    <scope>system</scope>
		  </dependency>
		  	<dependency>
		    <groupId>mms</groupId>
		    <artifactId>xmlParserAPIs</artifactId>
		    <version>1.0</version>
		    <systemPath>${basedir}/lib/xmlParserAPIs.jar</systemPath>
		    <scope>system</scope>
		  </dependency>
	</dependencies>

	<build>
		<finalName>mmsc-gateway</finalName>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-assembly-plugin</artifactId>
				<version>2.2.1</version>
				<configuration>
					<descriptors>
						<descriptor>src/main/assembly/assembly.xml</descriptor>
					</descriptors>
				</configuration>
				<executions>
					<execution>
						<id>make-assembly</id>
						<phase>package</phase>
						<goals>
							<goal>single</goal>
						</goals>
					</execution>
				</executions>
			</plugin>
		</plugins>
	</build>
</project>
