package com.unicom.mms.GraphicsMagick.operation;


/**
 * 
 * 根据一个图片或多个图片组合生成图片
 * 
 * @author zhangjh 新增日期：2013-5-7
 * @since mms-resources
 */
public class CompositeOperation extends Operation {
	
	/**
	 * 
	 * the background color
	 * 
	 * @param color
	 * @return
	 * @author zhangjh 新增日期：2013-5-7
	 * @since mms-resources
	 */
	public Operation addBackgroundColor(String color) {
		this.iCmdArgs.add("-background");
		this.iCmdArgs.add(color);
		return this;
	}
}
