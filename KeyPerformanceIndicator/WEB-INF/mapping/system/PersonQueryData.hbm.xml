<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.shengdai.kpi.system.favorite.vo.PersonQueryData" table="PSN_QUERY_PARA" lazy="true">
		
		<id name="id" column="id" type="string" > 
			<generator class="uuid.hex"/>
		</id>

		<property name="paraName" 
			column="PARA_NAME" 
			type="string"
		/>
		
		<property name="paraCode" 
			column="PARA_CODE" 
			type="string"
		/>
		<property name="paraShowValue" 
			column="PARA_SHOW_VALUE" 
			type="string"
		/>
		<property name="paraValue" 
			column="PARA_VALUE" 
			type="string"
		/>
		<property name="priority" 
			column="PRIORITY" 
			type="int"
		/>
		<many-to-one name="query" class="com.shengdai.kpi.system.favorite.vo.PersonQuery" fetch="select">
            	<column name="QUERY_ID" length="32" />
        	</many-to-one>      
		
	</class>
</hibernate-mapping>