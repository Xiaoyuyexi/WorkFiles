<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.shengdai.kpi.system.favorite.vo.PersonQueryScope" table="PSN_QUERY_SCOPE" lazy="true">
		
		<id name="id" column="id" type="string" > 
			<generator class="uuid.hex"/>
		</id>

		<property name="applyId" 
			column="object_id" 
			type="string"
		/>
		<property name="applyName" 
			column="object_name" 
			type="string"
		/>
		<property name="applyType" 
			column="object_type" 
			type="int"
		/>
		<property name="priority" 
			column="PRIORITY" 
			type="int"
		/>
		<many-to-one name="query" class="com.shengdai.kpi.system.favorite.vo.PersonQuery" fetch="select">
            	<column name="query_id" length="32" />
        	</many-to-one>      
		
	</class>
</hibernate-mapping>