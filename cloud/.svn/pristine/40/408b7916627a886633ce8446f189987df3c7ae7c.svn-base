<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd" >
<sqlMap namespace="cn.com.kxcomm.entity.OperatingSystemEntity">
	
	<resultMap id="ISOResultMap" class="cn.com.kxcomm.entity.OperatingSystemEntity">
		<result column="id" property="id" jdbcType="BIGINT" />
		<result column="system_name" property="systemName" jdbcType="VARCHAR" />
		<result column="median_type" property="medianType" jdbcType="VARCHAR" />
		<result column="version" property="version" jdbcType="VARCHAR" />
	</resultMap>

	<select id="queryAll" resultMap="ISOResultMap">
		select id,system_name,median_type,version from tb_operation_system
   </select>
  
</sqlMap>