    <sqlMap resource="cn/com/kxcomm/entity/DomUserEntity.xml"/>
    <sqlMap resource="cn/com/kxcomm/entity/GroupEntity.xml"/>
    <sqlMap resource="cn/com/kxcomm/entity/TenantsEntity.xml"/>
   <sqlMap resource="cn/com/kxcomm/entity/ConfigDetailEntity.xml"/>
   <sqlMap resource="cn/com/kxcomm/entity/ISOEntity.xml"/>
   <sqlMap resource="cn/com/kxcomm/entity/OperatingSystemEntity.xml"/>
   <sqlMap resource="cn/com/kxcomm/entity/OrderDetailEntity.xml"/>
   <sqlMap resource="cn/com/kxcomm/entity/RightEntity.xml"/>
   <sqlMap resource="cn/com/kxcomm/entity/RoleEntity.xml"/>
</sqlMapConfig>